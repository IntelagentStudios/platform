{
  "name": "@intelagent/database",
  "version": "1.0.0",
  "private": true,
  "description": "Multi-tenant database with schema isolation for Intelagent Platform",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "prisma generate && tsc",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "push": "prisma db push",
    "studio": "prisma studio",
    "seed": "node seed.js",
    "create-tenant": "node scripts/create-tenant.js",
    "init-db": "psql $DATABASE_URL -f prisma/tenant-schemas.sql",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@intelagent/redis": "file:../redis",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "prisma": "^5.0.0",
    "typescript": "^5.0.0"
  }
}