{
  "name": "intelagent-widget",
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/v1/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400, stale-while-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/latest/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600, stale-while-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/latest/chatbot.js",
      "destination": "/v1/chatbot.js"
    }
  ]
}